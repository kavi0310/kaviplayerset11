#include<stdio.h>
int main() {
   int i,a[1000],n,k=0,j,l,b[1000],c[1000],m=0,d[1000],p=0,sum=0,m1=0,p1=0;
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   for(i=0;i<n;i++)
   {
       b[k]=a[i];
       k++;
   }
   for(i=0;i<n;i++)
   {
       if(i!=0)
       {
           for(j=i-1;j<i;j++)
           {
               a[i]=a[i]+a[j];
           }
       }
       c[m]=a[i];
       m++;
       m1++;
   }
   for(k=n-1;k>=0;k--)
   {
        for(l=k-1;l>=0;l--)
        {
         b[k]=b[k]+b[l];
        }
        d[p]=b[k];
        p++;
        p1++;
   }
   for(m=0;m<m1;m++)
   {
       for(p=m;p<=p;p++)
       {
           sum=c[m]+d[p];
           break;
       }
       printf("%d ",sum);
   }
}
